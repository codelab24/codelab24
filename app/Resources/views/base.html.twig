<!DOCTYPE html>
<!--[if lt IE 7]>      <html class="no-js lt-ie9 lt-ie8 lt-ie7"> <![endif]-->
<!--[if IE 7]>         <html class="no-js lt-ie9 lt-ie8"> <![endif]-->
<!--[if IE 8]>         <html class="no-js lt-ie9"> <![endif]-->
<!--[if gt IE 8]><!--> <html class="no-js"> <!--<![endif]-->
<html>
    <head>
        <meta charset="utf-8">
        <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1">
        <meta name="description" content="">
        <meta name="viewport" content="width=device-width">
        <title>{% block title %}Welcome!{% endblock %}</title>

        {% block stylesheets %}
            <link href="{{ asset('/bundles/application/css/vendor/google-bootstrap/css/bootstrap.css') }}" rel="stylesheet" type="text/css" />
            <link rel="stylesheet" href="{{ asset('/bundles/application/css/vendor/google-bootstrap/css/bootstrap-responsive.css') }}" type="text/css" >
            <link rel="stylesheet" href="{{ asset('/bundles/application/css/vendor/google-bootstrap/css/todc-bootstrap.css') }}" type="text/css">
            <link rel="stylesheet" href="{{ asset('/bundles/application/css/vendor/font/icomoon.css') }}" type="text/css">
            <link rel="stylesheet" href="{{ asset('/bundles/application/css/vendor/font/opensans.css') }}" type="text/css">
            {#<link rel="stylesheet" href="{{ asset('/bundles/application/css/main.css') }}" type="text/css">#}
            <style>
                body {
                    padding-top: 60px;
                    padding-bottom: 40px;
                }
            </style>
            <link rel="stylesheet" href="{{ asset('/bundles/application/css/main.css') }}" type="text/css" >

        {% endblock %}
        <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
        <script src="{{ asset('/bundles/application/js/vendor/modernizr-2.6.2.min.js') }}" type="text/javascript"></script>
    </head>
    <body>
        
        {# Menu #}
        <div class="navbar navbar-fixed-top">
            <div class="navbar-inner">
                <a class="brand" href="#">Music Application</a>
                {{ knp_menu_render('ApplicationMainDefaultBundle:MenuBuilder:mainMenu', {'currentClass': 'active', 'template': 'ApplicationMainDefaultBundle:Menu:knp_menu.html.twig'}) }}
                {{ knp_menu_render('ApplicationMainDefaultBundle:MenuBuilder:userMenu', {'currentClass': 'active', 'template': 'ApplicationMainDefaultBundle:Menu:knp_menu.html.twig'}) }}
            </div>
        </div>
        {# End Menu#}
        <div class="container-fluid">
            <div class="row-fluid">
                <div class="span3">
                    options
                    {% block sidebar %}{% endblock %}
                </div>
                <div class="span3">
                    {% block body %}{% endblock %}
                </div>
            </div>
        </div>
        {#{% block body %}{% endblock %}#}
        {% block javascripts %}
            <script src="{{ asset('/bundles/application/js/vendor/jquery-1.9.1.min.js') }}" type="text/javascript"></script>
            <script src="{{ asset('/bundles/application/js/vendor/bootstrap.min.js') }}" type="text/javascript"></script>
        {% endblock %}
    </body>
</html>
